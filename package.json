{
  "name": "okclaire",
  "version": "0.2.0",
  "description": "Bot for Real Estate",
  "main": "dist/main.js",
  "private": true,
  "scripts": {
    "start": "node dist/main.js",
    "build": "babel src --source-maps --out-dir dist",
    "buildw": "babel src --source-maps --watch --out-dir dist",
    "watch": "npm run buildw & nodemon --delay 0.5 -w dist dist/main.js",
    "repl": "npm run buildw & nodemon --delay 0.5 -w dist dist/repl.js",
    "test": "npm run buildw & sleep 10 && ava --watch --verbose",
    "deploy": "npm run clean && npm run build && modulus deploy -p okclaire",
    "clean": "rimraf dist",
    "lint": "eslint src"
  },
  "dependencies": {
    "app-module-path": "1.1.0",
    "babel-runtime": "6.11.6",
    "bluebird": "3.4.3",
    "forecast-io": "1.0.2",
    "globby": "6.0.0",
    "googlemaps": "1.11.1",
    "got": "6.3.0",
    "is-url": "1.2.2",
    "koa": "2.0.0",
    "koa-router": "7.0.1",
    "mongoose": "4.7.8",
    "morgan": "1.7.0",
    "opbeat": "3.17.1",
    "raw-body": "2.1.7",
    "request": "2.74.0",
    "request-promise": "4.1.1",
    "source-map-support": "0.4.2",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "babel-cli": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-plugin-source-map-support-for-6": "0.0.5",
    "babel-plugin-syntax-flow": "6.13.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "babel-plugin-transform-promise-to-bluebird": "1.1.0",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-plugin-typecheck": "3.9.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.14.0",
    "babel-watch": "2.0.2",
    "eslint": "3.4.0",
    "eslint-config-standard": "6.0.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-flowtype": "2.11.4",
    "eslint-plugin-promise": "2.0.1",
    "eslint-plugin-standard": "2.0.0",
    "modulus": "6.3.1",
    "nock": "8.0.0",
    "nodemon": "1.10.2",
    "rimraf": "2.5.4",
    "supertest": "2.0.0",
    "supertest-as-promised": "4.0.0"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "source": [
      "dist/**/*.js"
    ],
    "concurrency": 5,
    "timeout": "10s",
    "require": [
      "babel-register",
      "./dist/test"
    ],
    "babel": "inherit"
  }
}
